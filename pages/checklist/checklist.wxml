<view class='item-list'>
  <view class='check-item' wx:for="{{proList}}" wx:key="{{item.uid}}" data-uid='{{item.uid}}' data-index='{{index}}'>
    <view class='weui-article__title'>{{item.name}} {{item.period}}届{{item.g3}}班</view>
    <view class="check-body">
      <view class="weui-media-box__desc">微信：{{item.wechat}}</view>
      <view class="weui-media-box__desc">手机：{{item.mobile}}</view>
      <view class="weui-media-box__desc">同班同学：{{item.classmates}}</view>
    </view>
    <van-button class="quick-check" type="info" size="small" data-check='{{item}}' catchtap='toQuickCheck'>快速审核</van-button>
  </view>
</view>

<view wx:if="{{!proList||!proList.length}}">
  <view class='ant-empty'>
    <image class='ant-empty__img' src="data:image/svg+xml;base64,"/>
  </view>
  <view class="ant-empty__description">暂无数据</view>
</view>


<view class="modal-mask" bindtap="hideModal" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>

<view class="modal-dialog shenhe-modal" wx:if="{{showModal}}">
  <form bindsubmit="onConfirm" report-submit="true">
    <view class="modal-title">校友审核</view>
    <view class="modal-content">
      <view class="modal-input">
        <radio-group  name="radio-group"  bindchange="radioChange">
          <label><radio value="1" color="#008DF8"/>通过</label>
          <label><radio value="0" color="#008DF8"/>不通过</label>
        </radio-group>
      </view>
    </view>
    <view class="modal-footer">
      <view class="btn-cancel" bindtap="onCancel" data-status="cancel">取消</view>
      <button class="btn-confirm" form-type="submit" data-status="confirm" data-checkData="{{checkData}}" data-checkItem="{{radioItem}}">确定</button>
      <!-- <view class="btn-confirm" bindtap="onConfirm" data-status="confirm" data-checkData="{{checkData}}" data-checkItem="{{radioItem}}">确定</view> -->
    </view>
  </form>
</view>
